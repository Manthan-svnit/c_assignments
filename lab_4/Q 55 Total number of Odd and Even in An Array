#include<stdio.h>
int main()
{
    int arr[10];
    int oddcount=0;
    int evencount=0;
    printf("Enter 10 numbers: \n");
    for(int i=0;i<10;i++)
    {
        scanf("%d",&arr[i]);
    }
    for(int i=0;i<10;i++)
    {
        if(arr[i]%2 != 0)
        {
            oddcount ++;
        }
        else
        {
            evencount ++;
        }
    }
    printf("Total number of oddcount is %d \n",oddcount);
    printf("Total number of evencount is %d \n", evencount);
}
