#include<stdio.h>
int main()
{
    int arr[10],i;
    int zero,positive,negative;
    zero=positive=negative=0;
    for(int i=0;i<10;i++)
    {
        scanf("%d \n",&arr[i]);
    }
    for(int i=0;i<10;i++)
    {
        printf("%d",arr[i]);
    }
    
    for(int i=0;i<10;i++);
    {
        if(arr[i]==0)
        {
            zero ++;
        }
        else if(arr[i]>0)
        {
            positive ++;
        }
        else if(arr[i]<0)
        {
            negative ++;
        }
    }
    printf("Number of positive elements are %d",positive);
    printf("Number of Negative Elements are %d",negative);
    printf("Number Of Zero Elements are %d",zero);
}
