#include<stdio.h>
int main()
{
    int arr[10];
    int sum=0;
            printf("Enter 10 numbers: \n");
    for(int i=0;i<10;i++)
    {
        scanf("%d",&arr[i]);
        printf("%d \n",arr[i]);
    }
    for(int i=0;i<10;i++)
    {
        sum = sum + arr[i];
    }
    printf("sum of numbers is %d", sum);
    return 0;
}
